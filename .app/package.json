{
  "name": "eleventy-notes",
  "version": "0.33.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm-run-all clean -p css:watch js:watch 11ty:serve",
    "build": "npm-run-all clean -p css:build js:build 11ty:build",
    "11ty:serve": "eleventy --serve",
    "11ty:build": "eleventy",
    "colors:generate": "node ./scripts/generate-colors.js",
    "css:watch": "node scripts/bundle-css.js --watch",
    "js:watch": "node scripts/bundle-js.js --watch",
    "css:build": "node scripts/bundle-css.js",
    "js:build": "node scripts/bundle-js.js",
    "clean": "node -e \"try { require('fs').rmSync('dist', {recursive: true, force: true, maxRetries: 3}); } catch(e) {}\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@parcel/transformer-sass": "^2.14.4",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-task-checkbox": "^1.0.6",
    "nanohtml": "^1.10.0",
    "node-html-parser": "^7.0.1",
    "npm-run-all2": "^8.0.1",
    "parcel": "^2.14.4",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "sass": "^1.87.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.5.1",
    "@radix-ui/colors": "^3.0.0",
    "alpinejs": "^3.14.9",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "eleventy-plugin-pwa-v2": "^1.0.2",
    "lucide": "^0.552.0",
    "pagefind": "^1.3.0",
    "prism-themes": "^1.9.0"
  },
  "browser": {
    "crypto": "crypto-browserify",
    "process": "process/browser",
    "buffer": "buffer",
    "stream": "stream-browserify",
    "events": "events",
    "util": "util",
    "assert": "assert",
    "vm": "./js/vm-stub.js",
    "fs": false,
